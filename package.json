  "notes": {
    "npm install --save": "this command will remove all the duplicate key notes, please git diff afterwards to restore them",
    "node-gyp": "OSX build failure fix: cd /usr/local/lib; sudo ln -s ../../lib/libSystem.B.dylib libgcc_s.10.5.dylib; sudo ln -s ../../lib/libSystem.B.dylib libgcc_s.10.4.dylib;",
    "typings.json": "TypeScript definitions for IDE autocompletion, installed using ```typings install```"
  },

  "scripts": "Mostly obsolete npm tasks, replaced by gulp tasks",
  "scripts": {
    "start":              "npm install; bower install; gulp",
    "postinstall":        "./node_modules/.bin/typings install; npm rebuild gulp-sass node-sass phantomjs; # modules with OS specific compilations",
    "production":         "gulp production",

    "test":               "npm run test:unit",
    "test:unit":          "karma start tests/karma.conf.js",
    "test:unit:once":     "karma start tests/karma.conf.js  --single-run",
    "test:e2e":           "npm run protractor",
    "preprotractor":      "webdriver-manager update",
    "protractor":         "killall protractor; killall chromedriver; killall java; protractor tests/protractor.conf.js",
  }
